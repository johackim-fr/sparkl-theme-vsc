{
	"name": "Sparkl",
	"type": "light",
	"colors": {
		"editor.background": "#f5f5f5",
		"editor.foreground": "#333"
	  },
	  "tokenColors": [
		{
		  "name": "Embedded",
		  "scope": [
			"source meta.tag",
			"source source.css",
			"source meta.embedded.block"
		  ],
		  "settings": {
			"foreground": "#2F74D0"
		  }
		},
		{
		  "name": "JSX embedded",
		  "scope": ["source meta.tag meta.embedded.expression"],
		  "settings": {
			"foreground": "#333"
		  }
		},
		{
		  "name": "Comments",
		  "scope": ["comment", "punctuation.definition.comment"],
		  "settings": {
			"fontStyle": "",
			"foreground": "#ababab"
		  }
		},
		{
		  "name": "Documentation",
		  "scope": [
			"comment.line.documentation",
			"comment.block.documentation",
			"comment.block.documentation punctuation.definition.comment"
		  ],
		  "settings": {
			"fontStyle": "",
			"foreground": "#62A9FF"
		  }
		},
		{
		  "name": "Exports",
		  "scope": "keyword.control.export",
		  "settings": {
			"foreground": "#fcb4ec",
			"fontStyle": ""
		  }
		},
		{
		  "name": "Invalid - Illegal",
		  "scope": "invalid.illegal",
		  "settings": {
			"foreground": "#FF4848"
		  }
		},
		{
		  "name": "Constants",
		  "scope": [
			"constant.language",
			"support.constant",
			"constant.numeric",
			"constant.character",
			"string"
		  ],
		  "settings": {
			"foreground": "#7451FF"
		  }
		},
		{
		  "name": "css-in-js constants",
		  "scope": [
			"source.js source.css constant.numeric",
			"source.js source.css constant.character",
			"source.js source.css constant",
			"source.js source.css string",
			"source.js source.css constant.language",
			"source.js source.css support.constant"
		  ],
		  "settings": {
			"foreground": "#7451FF"
		  }
		},
		{
		  "name": "Function Declarations",
		  "scope": [
			"entity.name.function",
			"entity.name.type",
			"support.function",
			"entity.other.inherited-class",
			"support.class"
		  ],
		  "settings": {
			"fontStyle": ""
		  }
		},
		{
		  "name": "Function usage",
		  "scope": [
			"meta.function-call entity.name.function",
			"meta.function-call support.function",
			"support.class.component"
		  ],
		  "settings": {
			"fontStyle": "bold"
		  }
		},
		{
		  "name": "Strings: Escape Sequences",
		  "scope": "constant.character.escape",
		  "settings": {
			"foreground": "#8668FF"
		  }
		},
		{
		  "name": "Template Strings",
		  "scope": ["meta.template.expression"],
		  "settings": {
			"foreground": "#333"
		  }
		},
		{
		  "name": "CSS Attribute Names",
		  "scope": ["source.css entity.other.attribute-name"],
		  "settings": {
			"fontStyle": "bold"
		  }
		}
	  ]
	}
	